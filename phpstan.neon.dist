includes:
    - phpstan-baseline.neon

parameters:
    level: 6
    paths:
        - src
        - tests
        - lib
        - migrations
    excludePaths:
        - lib/Nines/vendor
        - lib/Nines/*/DependencyInjection
        - src/Kernel.php
        - tests/*/data/*
        - tests/bootstrap.php
    treatPhpDocTypesAsCertain: false
